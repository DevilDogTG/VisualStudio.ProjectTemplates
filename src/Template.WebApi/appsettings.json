{
  "AppConfig": {
    "HttpsRedirect": false,
    "CorrelationKey": "X-Correlation-ID",
    "Logging": {
      "FileSize": 104857600,
      "FlushInterval": 1,
      "ConsoleLog": {
        "Enable": true,
        "MinimumLevel": "Information",
        "Template": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
      },
      "AppLog": {
        "Enable": true,
        "Path": "logs/logging..log",
        "Template": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz}][{Level:u3}][{MachineName}][{CorrelationId}] {Message:l}{NewLine}"
      },
      "AppJsonLog": {
        "EnableLogging": true,
        "Path": "logs/Json/logging..json"
      },
      "PerformanceLog": {
        "EnableLogging": true,
        "Path": "logs/Perf/logging-perf..json",
        "Template": "{Message:lj}{NewLine}"
      }
    }
  },
  "HotConfig": {
    "MaintainanceMode": false
  },
  "ConnectionStrings": {
    "Default": "Data Source=[Host];Initial Catalog=[DbName];User ID=[Uid];Password=[Pwd];TrustServerCertificate=True"
  }
}
